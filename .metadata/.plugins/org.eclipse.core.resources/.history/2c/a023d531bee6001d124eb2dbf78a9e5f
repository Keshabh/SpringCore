package com.springcore.StereotypeAnnotation;

import org.springframework.beans.factory.annotation.Value;
import org.springframework.stereotype.Component;

@Component
public class Spel {
	@Value("#{4+5}")
	private int x;
	@Value("#T(java.lang.Math).sqrt(144)")
	private int b;
	@Value("#{a+b}")
	private int result;
	
	
}
